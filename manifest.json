{
    "manifest_version": 3,
    "name": "La NotiF",
    "version": "6.9.419",
    "description": "Icône colorée = live. Icône grise = pas de live. Basique. Simple.",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "64" : "img/icon_red.png",
        "128": "img/icon_app.png"
    },
    "permissions": [
        "identity",
        "notifications"
    ]
}